
pipeline {
    agent any

    tools {
        gradle 'Gradle'
    }

    stages {
        stage('Tareas de limpieza') {
            steps {
                git 'https://github.com/CristianRosasAyala/FreeRangeAutPractice'
                sh 'gradle clean'
            }
        }
        stage('Imprimo cositas') {
            steps {
                echo "Soy un stage aparte"
            }
        }
    }
}
